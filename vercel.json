{
  \"builds\": [
    {
      \"src\": \"package.json\",
      \"use\": \"@vercel/static-build\",
      \"config\": { 
        \"distDir\": \"dist\"
      }
    }
  ],
  \"routes\": [
    {
      \"src\": \"/(.*)\",
      \"dest\": \"/dist/\"
    }
  ],
  \"env\": {
    \"VITE_API_URL\": \"@vite_api_url\"
  }
}
